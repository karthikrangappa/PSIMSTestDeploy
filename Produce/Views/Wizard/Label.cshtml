@using Intelledox.Produce
@model Intelledox.ViewModel.Core.QuestionViewModel<Intelledox.QAWizard.LabelQuestion>
@{ 
    Layout = "~/Views/Shared/_QuestionLayout.cshtml";
    var value = Model.Question.Value;
}
<div id="@Html.IdFor(Model.Question)" class="question-label">@Html.Raw(HtmlParsing.Sanitize(value != null ? value.ToString() : ""))</div>