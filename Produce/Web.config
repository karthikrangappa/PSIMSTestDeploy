<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="glimpse" type="Glimpse.Core.Configuration.Section, Glimpse.Core" />
  </configSections>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target xsi:type="Database" name="Logger" keepConnection="true" dbDatabase="Logger" commandText="INSERT INTO AuditLog (DateCreatedUtc, IPAddress, Event, UserGuid, UserName, ExtraDetails, LoginTimeUtc) values (@dateCreatedUtc, @ipAddress, @event, @userGuid, @userName, @extraDetails, NULLIF(@loginTimeUtc,''))" connectionString="">
        <parameter name="@dateCreatedUtc" layout="${event-context:item=dateCreatedUtc}" />
        <parameter name="@ipAddress" layout="${event-context:item=ipAddress}" />
        <parameter name="@event" layout="${message}" />
        <parameter name="@userGuid" layout="${event-context:item=userGuid}" />
        <parameter name="@userName" layout="${event-context:item=userName}" />
        <parameter name="@extraDetails" layout="${event-context:item=extraDetails}" />
        <parameter name="@loginTimeUtc" layout="${event-context:item=loginTimeUtc}" />
      </target>
    </targets>
    <rules>
      <logger name="*" minlevel="Info" writeTo="Logger" />
    </rules>
  </nlog>
  <glimpse defaultRuntimePolicy="Off" endpointBaseUri="~/Glimpse.axd">
    <tabs>
      <ignoredTypes>
        <add type="Glimpse.AspNet.Tab.Session, Glimpse.AspNet" />
        <add type="Glimpse.AspNet.Tab.Configuration, Glimpse.AspNet" />
        <add type="Glimpse.AspNet.Tab.Environment, Glimpse.AspNet" />
        <add type="Glimpse.AspNet.Tab.Server, Glimpse.AspNet" />
        <add type="Glimpse.AspNet.Tab.Cache, Glimpse.AspNet" />
      </ignoredTypes>
    </tabs>
  </glimpse>
  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
    <!-- Text which shold be displayed in back/home button-->
    <add key="BackToDashboardText" value="Back to Dashboard" />
    <!-- MainDashboardID, this is published main dashboard Guid -->
    <add key="MainDashboardID" value="" />
    
    <add key="ProducedProjectPath" value="https://permitsonlinedev.azurewebsites.net/Produce/wizard/{0}/" />
  </appSettings>
  <connectionStrings>
    <clear />
    <add name="ConnectionString" connectionString="ix" providerName="ix" />
  </connectionStrings>
  <system.web>
    <compilation defaultLanguage="vb" debug="false" targetFramework="4.6.2">
      <assemblies>
        <add assembly="System.Runtime, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
      </assemblies>
    </compilation>
    <customErrors mode="Off" />
    <authentication mode="None" />
    <membership>
      <providers>
        <clear />
      </providers>
    </membership>
    <machineKey validationKey="E2331A45A334C586AA384F510BD52D960E865BA877314DDEC92AE8322AF1CF656AD845BB9F19B4AB57BC703D85EBFE5CC2B4C8AFA7EAFF7049B8E995EEBDD3A7" decryptionKey="EC25B2AE0C71034C604BB4E4FCA93E76891A061CBEDF0706" validation="SHA1" />
    <pages validateRequest="false" clientIDMode="AutoID" enableViewState="true" />
    <!-- Request length is set to 20MB so that large files can be imported  -->
    <httpRuntime maxRequestLength="20000" requestValidationMode="2.0" enableVersionHeader="false" targetFramework="4.6.2" sendCacheControlHeader="false" />
    <httpHandlers>
    </httpHandlers>
    <globalization requestEncoding="utf-8" responseEncoding="utf-8" />
    <httpCookies requireSSL="false" />
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <staticContent>
      <clientCache cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" />
      <remove fileExtension=".json" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <remove fileExtension=".woff" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
      <remove fileExtension=".woff2" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff" />
      <remove fileExtension=".apk" />
      <mimeMap fileExtension=".apk" mimeType="application/vnd.android.package-archive" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="Glimpse" path="glimpse.axd" verb="GET" type="Glimpse.AspNet.HttpHandler, Glimpse.AspNet" preCondition="integratedMode" />
    </handlers>
    <modules>
      <add name="Glimpse" type="Glimpse.AspNet.HttpModule, Glimpse.AspNet" preCondition="integratedMode" />
      <remove name="FormsAuthentication" />
    </modules>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
        <add name="X-Frame-Options" value="SAMEORIGIN" />
        <add name="X-XSS-Protection" value="1; mode=block" />
        <add name="X-Content-Type-Options" value="nosniff" />
      </customHeaders>
    </httpProtocol>
    <security>
      <requestFiltering>
        <hiddenSegments>
          <add segment="appsettings.json" />
        </hiddenSegments>
        <requestLimits maxAllowedContentLength="20000000" />
      </requestFiltering>
    </security>
  </system.webServer>
  <location path="WizardSupport/SelectFileUpload">
    <system.web>
      <httpRuntime executionTimeout="1800" />
    </system.web>
  </location>
  <location path="Tools">
    <system.webServer>
      <staticContent>
        <remove fileExtension=".exe" />
      </staticContent>
    </system.webServer>
  </location>
  <system.net>
    <mailSettings>
      <smtp deliveryMethod="Network">
        <network host="127.0.0.1" />
      </smtp>
    </mailSettings>
  </system.net>
